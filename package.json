{
	"name": "musicore",
	"version": "0.0.1",
	"description": "Manipulate music scores, render sheet music, music theory exercises and more.",
	"keywords": [
		"music",
		"score",
		"sheet music",
		"music theory",
		"music exercises",
		"musicstring"
	],
	"homepage": "https://marcus.ax",
	"licence": "MIT",
	"author": "Marcus Gustafsson <marcus@marcus.ax> (https://marcus.ax)",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "vite dev",
		"build": "vite build && pnpm run package",
		"build:js": "node esbuild.js",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "pnpm run package",
		"test": "pnpm run test:integration && pnpm run test:unit",
		"check": "svelte-kit sync && svelte-check  --compiler-warnings 'missing-custom-element-compile-options:ignore' --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"u": "pnpm update --interactive"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.0.0-next.258"
	},
	"devDependencies": {
		"@playwright/test": "^1.48.2",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.7.4",
		"@sveltejs/package": "^2.3.7",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^9.6.1",
		"autoprefixer": "^10.4.20",
		"esbuild": "^0.24.0",
		"esbuild-style-plugin": "^1.6.3",
		"esbuild-svelte": "^0.8.2",
		"eslint": "^9.14.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.11.0",
		"happy-dom": "^15.8.2",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.7",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"publint": "^0.2.12",
		"svelte": "^5.1.9",
		"svelte-check": "^4.0.5",
		"svelte-preprocess": "^6.0.3",
		"tailwindcss": "^3.4.14",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.12.2",
		"vite": "^5.4.10",
		"vitest": "^2.1.4"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"abcjs": "^6.4.4",
		"fraction.js": "^4.3.7"
	}
}
